{
	"compilerOptions": {
		"target": "es2018",
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"lib": [
			"es2018",
			"dom"
		],
		"noImplicitAny": false,
		"suppressImplicitAnyIndexErrors": true,
		"removeComments": false,
		"outDir": "dist"
	}
}